SELECT 
  t1.student_id 
FROM
  (SELECT 
    student_id,
    num 
  FROM
    score 
  WHERE course_id = 
    (SELECT 
      cid 
    FROM
      course 
    WHERE cname = 'ç‰©ç†')) AS t1 
  INNER JOIN 
    (SELECT 
      student_id,
      num 
    FROM
      score 
    WHERE course_id = 
      (SELECT 
        cid 
      FROM
        course 
      WHERE cname = 'ç”Ÿç‰©')) AS t2 
    ON t1.student_id = t2.student_id 
WHERE t1.num > t2.num ;